.leftgrid table,.rightgrid table{border-collapse:collapse;border:1px solid #000}.leftgrid table thead,.rightgrid table thead{border-bottom:none}.leftgrid table thead th,.rightgrid table thead th{height:60px;vertical-align:top;font-size:12px;border:1px solid #000;border-bottom:none;background:#daded5}.leftgrid{float:left;width:50px}.leftgrid .top{border-right:none;border-bottom:none}.leftgrid .top th{background:#fff;border-right:none;border-bottom:none;font-size:12px}.leftgrid table{width:100%}.leftgrid .bottom{overflow-x:scroll;overflow-y:hidden}.leftgrid .bottom table{border-right:none}.leftgrid .bottom table tbody td{vertical-align:middle;background:#bfeb8f;border:1px solid #000;position:relative;border-bottom:none;border-top:none;font-size:12px;height:25px;text-align:right;border-right:none}.rightgrid{float:left}.rightgrid table{border-left:none;border-top:none}.rightgrid table tbody td{font-size:12px;text-align:right;height:25px;border:1px solid #000;position:relative;background:url(../images/dashed.png) repeat-x 0 center;border-bottom:none;border-top:none}.rightgrid table tbody td .section{position:absolute;width:100%;background:#53b119;margin-top:13px;z-index:90;opacity:.8;text-align:center}